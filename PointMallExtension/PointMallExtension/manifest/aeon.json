{
  "name": "イオンカード ときめきポイントTOWN",
  "description": "イオンカードユーザーのための拡張機能",
  "version": "1.0.0.0",
  "author": "MIZUNE Pine",
  "manifest_version": 2,
  "icons": {
    "48": "assets/icons/icon48.png",
    "128": "assets/icons/icon128.png"
  },
  "background": {
    "scripts": [
	  "vendor/js/zepto.min.js",
      "vendor/js/underscore-min.js",
      "vendor/js/url.min.js",
      "common/common.js",
	  "background/background.js"
    ]
  },
  "content_scripts": [
  ],
  "page_action": {
    "default_icon": "assets/icons/icon19.png",
    "default_title": "ときめきポイントTOWN",
    "default_popup": "page_action/popup.html"
  },
  "web_accessible_resources": [
    "assets/images/*",
    "vendor/js/*"
  ],
  "permissions": [
    "storage",
    "tabs",
    "*://*/*"
  ]
}
